      "request": {
        "method": "POST",
        "urlPath": "/v1/accounts:lookup",
        "bodyPatterns": [
          {
            "contains": "invalid-token"
          }
        ]
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/json"
