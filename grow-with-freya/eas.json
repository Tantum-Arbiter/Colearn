{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "NODE_ENV": "development"
      },
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.growwithfreya.app.dev"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "applicationId": "com.growwithfreya.app.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_ENV": "staging"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true,
        "bundleIdentifier": "com.growwithfreya.app.preview"
      },
      "android": {
        "buildType": "apk",
        "applicationId": "com.growwithfreya.app.preview"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.growwithfreya.app"
      },
      "android": {
        "buildType": "aab",
        "applicationId": "com.growwithfreya.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
