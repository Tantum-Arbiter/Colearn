# Environment Configuration for Grow with Freya
# Copy this file to .env and fill in your values

# Expo Configuration
EXPO_TOKEN=your_expo_access_token_here

# Build Configuration
NODE_ENV=development

# API Configuration (if needed)
API_BASE_URL=https://api.growwithfreya.com
API_TIMEOUT=10000

# Analytics Configuration (if needed)
ANALYTICS_ENABLED=false
ANALYTICS_API_KEY=your_analytics_key_here

# Feature Flags
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_MODE=true

# App Store Configuration
IOS_BUNDLE_ID=com.growwithfreya.app
ANDROID_PACKAGE_NAME=com.growwithfreya.app

# Development Configuration
DEV_IOS_BUNDLE_ID=com.growwithfreya.app.dev
DEV_ANDROID_PACKAGE_NAME=com.growwithfreya.app.dev

# Preview Configuration
PREVIEW_IOS_BUNDLE_ID=com.growwithfreya.app.preview
PREVIEW_ANDROID_PACKAGE_NAME=com.growwithfreya.app.preview
